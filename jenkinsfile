pipeline{
    agent any

    stages
    {
        stage("develop")
        {
            steps{
            bat 'javac Main.java'
            }
        }

        stage("run")
        {
            steps{
            bat 'java Main'
            }
        }
    }
    post
    {
        always {
            bat 'echo "always"'
        }

        sucess{
            bat 'echo "sucess"'
        }

        failure{
            bat 'echo "failure"'
        }
    }
}